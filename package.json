{
  "name": "shadrack-family-website",
  "version": "2.0.0",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "build-css": "postcss public/assets/css/styles.css -o public/assets/css/styles.min.css",
    "build-js": "uglifyjs public/assets/js/main.js -o public/assets/js/main.min.js",
    "build": "npm run build-css && npm run build-js",
    "prestart": "npm run build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.37.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf-csrf": "^4.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.5",
    "uglify-js": "^3.17.4"
  }
}
